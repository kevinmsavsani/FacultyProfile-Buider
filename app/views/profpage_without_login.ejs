
<!-- nav bar -->
<link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.6/css/bootstrap.min.css" integrity="sha384-1q8mTJOASx8j1Au+a5WDVnPi2lkFfwwEAa8hDDdjZlpLegxhjVME1fgjWPGmkzs7" crossorigin="anonymous">
<link href="https://maxcdn.bootstrapcdn.com/font-awesome/4.3.0/css/font-awesome.min.css" rel="stylesheet">
<link href='https://fonts.googleapis.com/css?family=Open+Sans|Roboto+Slab' rel='stylesheet' type='text/css'>

<link rel='stylesheet' href='/css/navbar.css' />


<!-- nav bar -->
<link href="https://maxcdn.bootstrapcdn.com/font-awesome/4.3.0/css/font-awesome.min.css" rel="stylesheet">
<%include profileNav.ejs%>
<div class="container">
    <div class="row">
        <%include navbar.ejs%>

        <div class="col-sm-9">

            <!-- resumt -->
            <div class="panel panel-default">
                <div class="panel-heading resume-heading">
                    <div class="row">
                        <div class="col-lg-12">
                            <div class="col-xs-12 col-sm-4">
                                <figure>
                                    <img src="http://graph.facebook.com/<%= users.facebookID%>/picture?width=150" class="img-circle" alt="User Image" />
                                </figure>
                                <div class="row">
                                    <div class="col-xs-12 social-btns">
                                        <div class="col-xs-3 col-md-1 col-lg-1 social-btn-holder">
                                            <a href="goo.gl/7wR3Zx" class="btn btn-social btn-block btn-google">
                                                <i class="fa fa-google"></i> </a>
                                        </div>
                                        <div class="col-xs-3 col-md-1 col-lg-1 social-btn-holder">
                                            <a href="https://www.facebook.com/" class="btn btn-social btn-block btn-facebook">
                                                <i class="fa fa-facebook"></i> </a>
                                        </div>
                                        <div class="col-xs-3 col-md-1 col-lg-1 social-btn-holder">
                                            <a href="https://mobile.twitter.com/login" class="btn btn-social btn-block btn-twitter">
                                                <i class="fa fa-twitter"></i> </a>
                                        </div>
                                        <div class="col-xs-3 col-md-1 col-lg-1 social-btn-holder">
                                            <a href="https://in.linkedin.com/" class="btn btn-social btn-block btn-linkedin">
                                                <i class="fa fa-linkedin"></i> </a>
                                        </div>
                                        <div class="col-xs-3 col-md-1 col-lg-1 social-btn-holder">
                                            <a href="https://github.com/" class="btn btn-social btn-block btn-github">
                                                <i class="fa fa-github"></i> </a>
                                        </div>
                                        <div class="col-xs-3 col-md-1 col-lg-1 social-btn-holder">
                                            <a href="https://stackoverflow.com/" class="btn btn-social btn-block btn-stackoverflow">
                                                <i class="fa fa-stack-overflow"></i> </a>
                                        </div>
                                    </div>


                                </div>
                            </div>
                            <div class="col-xs-12 col-sm-8">
                                <ul class="list-group">

                                    <li class="list-group-item"><i class="glyphicon glyphicon-user"></i> <%=users.name%> </li>
                                    <li class="list-group-item"><i class="glyphicon glyphicon-sound-dolby"></i> <%=users.department%></li>
                                    <li class="list-group-item"><i class="glyphicon glyphicon-globe"></i> <%=users.room%> </li>
                                    <li class="list-group-item"><i class="fa fa-phone"></i> <%=users.contactNo%> </li>
                                    <li class="list-group-item"><i class="fa fa-envelope"></i> <%=users.email%></li>

                                </ul>
                            </div>
                        </div>
                    </div>
                </div>
                <div class="bs-callout bs-callout-danger">
                    <h4>Education Qualifications:</h4>
                    <p>
                        <%=users.education%>
                    </p>

                </div>
                <div class="bs-callout bs-callout-danger">
                    <h4>Research Interests:</h4>
                    <p>
                        <%=users.research_interest%>
                    </p>
                </div>
                <div class="bs-callout bs-callout-danger">
                    <h4>Experiences:</h4>
                    <p>
                        <%=users.experience%>
                    </p>
                </div>

            </div>
        </div>
        <!-- resume -->

    </div>
</div>
